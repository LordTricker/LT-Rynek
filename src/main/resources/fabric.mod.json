{
	"schemaVersion": 1,
	"id": "ltrynek",
	"version": "${version}",
	"name": "LT-Rynek",
	"description": "Amatorska modyfikacja ułatwiająca działania rynkowe na różnych serwerach. Jeżeli powielasz, to miłoby było jakbyś podzielił się info na kim bazujesz <3.",
	"authors": [
		"LordTricker & Mr. GPT"
	],
	"contact": {
		"homepage": "https://blazecode.pl/",
		"sources": "https://github.com/LordTricker"
	},
	"license": "CC0-1.0",
	"icon": "assets/ltrynek/icon.png",

	"environment": "*",
	"entrypoints": {
		"client": [
			"com.example.ltrynek.client.LtrynekClient"
		]
	},
	"mixins": [
		"modid.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],

	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
